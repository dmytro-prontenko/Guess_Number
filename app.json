{
  "expo": {
    "name": "Guess_Number",
    "slug": "Guess_Number",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.webp",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-font",
        {
          "fonts": ["path/to/file.ttf"]
        }
      ]
    ],

    "splash": {
      "image": "./assets/splash.webp",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.webp",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.webp"
    }
  }
}
